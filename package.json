{
  "name": "skill-connect",
  "version": "1.0.0",
  "description": "Skill Connect - User Management and Collaboration Platform",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm --prefix backend run dev\" \"npm --prefix frontend start\"",
    "start": "concurrently \"npm --prefix backend start\" \"npm --prefix frontend start\"",
    "backend": "npm --prefix backend start",
    "frontend": "npm --prefix frontend start",
    "build": "npm --prefix backend run build && npm --prefix frontend run build",
    "docker-up": "docker-compose up -d",
    "docker-down": "docker-compose down",
    "docker-logs": "docker-compose logs -f"
  },
  "dependencies": {
    "axios": "^1.13.4",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-fileupload": "^1.5.2",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.563.0",
    "pg": "^8.18.0",
    "react": "^19.2.4",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
